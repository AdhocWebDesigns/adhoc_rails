<p id="notice"><%= notice %></p>

<h1>Weapons</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>W type</th>
      <th>Name</th>
      <th>Description</th>
      <th>Damage</th>
      <th>D type</th>
      <th>Weight</th>
      <th>Value</th>
      <th>W prop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weapons.each do |weapon| %>
      <tr>
        <td><%= weapon.user %></td>
        <td><%= weapon.w_type %></td>
        <td><%= weapon.name %></td>
        <td><%= weapon.description %></td>
        <td><%= weapon.damage %></td>
        <td><%= weapon.d_type %></td>
        <td><%= weapon.weight %></td>
        <td><%= weapon.value %></td>
        <td><%= weapon.w_prop %></td>
        <td><%= link_to 'Show', weapon %></td>
        <td><%= link_to 'Edit', edit_weapon_path(weapon) %></td>
        <td><%= link_to 'Destroy', weapon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weapon', new_weapon_path %>
