<p id="notice"><%= notice %></p>

<h1>Spell Schools</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spell_schools.each do |spell_school| %>
      <tr>
        <td><%= spell_school.name %></td>
        <td><%= spell_school.desc %></td>
        <td><%= link_to 'Show', spell_school %></td>
        <td><%= link_to 'Edit', edit_spell_school_path(spell_school) %></td>
        <td><%= link_to 'Destroy', spell_school, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spell School', new_spell_school_path %>
