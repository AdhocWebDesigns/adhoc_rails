<p id="notice"><%= notice %></p>

<h1>Spell Component Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Symbol</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spell_component_types.each do |spell_component_type| %>
      <tr>
        <td><%= spell_component_type.name %></td>
        <td><%= spell_component_type.symbol %></td>
        <td><%= spell_component_type.desc %></td>
        <td><%= link_to 'Show', spell_component_type %></td>
        <td><%= link_to 'Edit', edit_spell_component_type_path(spell_component_type) %></td>
        <td><%= link_to 'Destroy', spell_component_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Spell Component Type', new_spell_component_type_path %>
