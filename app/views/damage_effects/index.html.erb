<p id="notice"><%= notice %></p>

<h1>Damage Effects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @damage_effects.each do |damage_effect| %>
      <tr>
        <td><%= damage_effect.name %></td>
        <td><%= damage_effect.value %></td>
        <td><%= link_to 'Show', damage_effect %></td>
        <td><%= link_to 'Edit', edit_damage_effect_path(damage_effect) %></td>
        <td><%= link_to 'Destroy', damage_effect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Damage Effect', new_damage_effect_path %>
